{
  "name": "trek-body-parser",
  "version": "0.1.4",
  "description": "Body parsing middleware",
  "repository": "trekjs/body-parser",
  "license": "MIT",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "fundon.me"
  },
  "keywords": [
    "trek",
    "engine",
    "middleware",
    "body-parser",
    "json",
    "urlencoded",
    "multipart",
    "body"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "xo",
    "test": "xo && nyc ava"
  },
  "dependencies": {
    "busboy": "0.2.14",
    "inflation": "2.0.0",
    "multer": "2.0.0-alpha.2",
    "qs": "6.3.0",
    "raw-body": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.18",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-es2015-modules-commonjs": "6",
    "babel-preset-es2016": "6",
    "babel-register": "6",
    "nyc": "10",
    "request": "2",
    "request-promise": "4",
    "trek-engine": "0.5",
    "xo": "0.17"
  },
  "ava": {
    "concurrency": 5,
    "require": "babel-register"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs"
    ],
    "presets": [
      "es2016"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": true,
    "ignores": [],
    "rules": {
      "curly": 0,
      "import/no-dynamic-require": 0,
      "import/no-named-as-default": 0,
      "max-lines": 0,
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "todo",
            "fixme"
          ],
          "location": "anywhere"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": true,
          "objectsInObjects": true
        }
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "yoda": 0
    }
  }
}
