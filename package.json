{
  "name": "trek-body-parser",
  "version": "0.2.2",
  "description": "Body parsing middleware",
  "repository": "trekjs/body-parser",
  "license": "MIT",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "fundon.me"
  },
  "keywords": [
    "trek",
    "engine",
    "middleware",
    "body-parser",
    "json",
    "urlencoded",
    "multipart",
    "body"
  ],
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "lint": "xo",
    "precommit": "lint-staged",
    "test": "xo && nyc ava"
  },
  "dependencies": {
    "busboy": "0.2.14",
    "inflation": "2.0.0",
    "multer": "2.0.0-alpha.6",
    "qs": "6.5.0",
    "raw-body": "2.3.1"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "eslint-config-prettier": "2.4.0",
    "husky": "0.14.3",
    "lint-staged": "4.1.3",
    "nyc": "11.2.1",
    "prettier": "1.6.1",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "trek-engine": "1",
    "xo": "0.18.2"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/**/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "xo",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "xo": {
    "extends": "prettier"
  }
}
